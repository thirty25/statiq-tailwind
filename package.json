  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.3",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "tailwindcss": "^2.0.2"
  }
}
